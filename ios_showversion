---
- name: show run
  hosts: switch
  connection: local
  gather_facts: no
  
  vars:
    cli:
      host: "{{ ansible_host }}"
      username: hkt
      password: Root123#
      authorize: yes
  tasks:
    - name: show running-config
      ios_command:
        commands:
             - show version
        provider: "{{ cli }}"
      register: show run
    
    - name: msg
      debug:
          msg: "{{ show_run.stdout_lines }}"
